@model Nereus.ViewModels.CreateOrEditProjectViewModel

@{
    ViewBag.Title = "Create Project";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Styles.Render("~/Content/Common/Common.css")

<div id="topBar">
    <h2 class="pageBody">Create Project</h2>
</div>

<script src="@Url.Content("~/Scripts/Common/ThirdParty/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Common/ThirdParty/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

<div class="pageBody" style="margin-top: 10px;">
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset id="createProject">
        <legend id="projectName">Project</legend>

		@Html.Partial("_CreateOrEdit", Model)

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

</div>


